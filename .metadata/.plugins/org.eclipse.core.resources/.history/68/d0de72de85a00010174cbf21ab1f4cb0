

package ejercicio1;

import java.util.Scanner;

public class Ejercicio1 {

	public static void main(String[] args) {
		Scanner teclado = new Scanner(System.in);
		
		double num1 ;
		double num2 ;
		
		System.out.print("Escribe el valor del num1: ");
		num1 = teclado.nextDouble();
		System.out.print("Escribe el valor del num2: ");
		num2 = teclado.nextDouble();
		
		if (num1 < num2){ System.out.print("El num2 es el grande: "); }
		else if (num1 == num2) {System.out.print("Son iguales");}
		else { System.out.println("El num1 es el Grande"); }
	}

}
